{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "config": {
    "session":{
      "userSessionName":"user-session",	
      "sessionOptions":{
    	  "name": "test-custom-auth",
    	  "secret": "my custom auth secret",
    	  "saveUninitialized": true,
    	  "resave": true
      },
	  "fileStoreOptions": {"path":"./sessions"}
	}
  }
}
